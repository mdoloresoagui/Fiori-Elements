<edmx:Edmx xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx" Version="4.0">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Common.v1" Alias="Common" />
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/UI.xml">
        <edmx:Include Namespace="com.sap.vocabularies.UI.v1" Alias="UI" />
    </edmx:Reference>
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Communication.xml">
        <edmx:Include Namespace="com.sap.vocabularies.Communication.v1" Alias="Communication" />
    </edmx:Reference>
    <edmx:Reference Uri="/v2/northwind/northwind.svc/$metadata">
        <edmx:Include Namespace="NorthwindModel" />
        <edmx:Include Namespace="ODataWeb.Northwind.Model" />
    </edmx:Reference>
    <edmx:DataServices>
        <Schema xmlns="http://docs.oasis-open.org/odata/ns/edm" Namespace="local">
            <!--Table cards-->
            <Annotations Target="NorthwindModel.Product">
                <!--Line Item-->
                <Annotation Term="UI.LineItem" Qualifier="Products">
                    <Collection>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="Supplier/CompanyName" />
                            <PropertyValue Property="Label" String="{@i18n>supplier}" />
                            <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="Category/CategoryName" />
                            <PropertyValue Property="Label" String="{@i18n>category}" />
                            <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="ProductName" />
                            <PropertyValue Property="Label" String="{@i18n>product}" />
                            <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="UnitsInStock" />
                            <PropertyValue Property="Label" String="{@i18n>unitsInStock}" />
                            <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/High" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="QuantityPerUnit" />
                            <PropertyValue Property="Label" String="{@i18n>quantityPerUnit}" />
                            <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/Low" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="UnitPrice" />
                            <PropertyValue Property="Label" String="{@i18n>unitPrice}" />
                            <Annotation Term="UI.Importance" EnumMember="UI.ImportanceType/Low" />
                        </Record>
                    </Collection>
                </Annotation>
                <!--Data Point-->
                <Annotation Term="UI.DataPoint" Qualifier="ProductsDataPoint">
                    <Record Type="UI.DataPointType">
                        <PropertyValue Property="Title" String="{@i18n>stock}" />
                        <PropertyValue Property="Description" String="{@i18n>unitsInStock}" />
                        <PropertyValue Property="Value" Path="UnitsInStock" />
                        <PropertyValue Property="CriticalityCalculation">
                            <Record Type="UI.CriticalityCalculationType">
                                <PropertyValue Property="ImprovementDirection" EnumMember="UI.ImprovementDirectionType/Maximize" />
                                <PropertyValue Property="ToleranceRangeLowValue" Path="ReorderLevel" />
                                <PropertyValue Property="DeviationRangeLowValue" Path="ReorderLevel" />
                            </Record>
                        </PropertyValue>
                    </Record>
                </Annotation>
            </Annotations>
            <!--List cards-->
            <Annotations Target="NorthwindModel.Customer">
                <Annotation Term="UI.LineItem" Qualifier="ListCustomer">
                    <Collection>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="CompanyName" />
                            <PropertyValue Property="Label" String="{@i18n>companyName}" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="ContactName" />
                            <PropertyValue Property="Label" String="{@i18n>contactName}" />
                        </Record>
                        <Record Type="UI.DataField">
                            <PropertyValue Property="Value" Path="Address" />
                            <PropertyValue Property="Label" String="{@i18n>address}" />
                        </Record>
                    </Collection>
                </Annotation>
            </Annotations>
            <!--Stack (pila) cards-->
            <Annotations Target="NorthwindModel.Order">
                <Annotation Term="UI.HeaderInfo">
                    <Record Type="UI.HeaderInfoType">
                        <PropertyValue Property="TypeName" String="{@i18n>order}" />
                        <PropertyValue Property="TypeNamePlural" String="{@i18n>orders}" />

                        <PropertyValue Property="Title">
                            <Record Type="UI.DataField">
                                <PropertyValue Property="Value" Path="OrderID" />
                                <PropertyValue Property="Label" String="{@i18n>orderId}" />
                            </Record>
                        </PropertyValue>

                        <PropertyValue Property="ImageUrl" String="sap-icon://sales-order" />

                        <PropertyValue Property="Description">
                            <Record Type="UI.DataField">
                                <PropertyValue Property="Value" Path="OrderDate" />
                                <PropertyValue Property="Label" String="{@i18n>orderDate}" />
                            </Record>
                        </PropertyValue>
                    </Record>
                </Annotation>
                <!--Field Group-->
                <Annotation Term="UI.FieldGroup" Qualifier="Group_Order">
                    <Record Type="UI.FieldGroupType">
                        <PropertyValue Property="Data">
                            <Collection>
                                <Record Type="UI.DataField">
                                    <PropertyValue Property="Value" Path="ShipName" />
                                    <PropertyValue Property="Label" String="{@i18n>shipName}" />
                                </Record>
                                <Record Type="UI.DataField">
                                    <PropertyValue Property="Value" Path="ShipCountry" />
                                    <PropertyValue Property="Label" String="{@i18n>shipCountry}" />
                                </Record>
                                <Record Type="UI.DataField">
                                    <PropertyValue Property="Value" Path="ShipRegion" />
                                    <PropertyValue Property="Label" String="{@i18n>shipRegion}" />
                                </Record>
                                <Record Type="UI.DataField">
                                    <PropertyValue Property="Value" Path="ShipCity" />
                                    <PropertyValue Property="Label" String="{@i18n>shipCity}" />
                                </Record>
                                <Record Type="UI.DataField">
                                    <PropertyValue Property="Value" Path="ShipAddress" />
                                    <PropertyValue Property="Label" String="{@i18n>shipAddress}" />
                                </Record>
                            </Collection>
                        </PropertyValue>
                    </Record>
                </Annotation>
                <!--Facets-->
                <Annotation Term="UI.Facets" Qualifier="ContainerOrder">
                    <Collection>
                        <Record Type="UI.ReferenceFacet">
                            <PropertyValue Property="Target" AnnotationPath="@UI.FieldGroup#Group_Order" />
                        </Record>
                    </Collection>
                </Annotation>
            </Annotations>
            <!--Link List card-->
            <Annotations Target="NorthwindModel.Employee" Qualifier="Header">
                <Annotation Term="UI.HeaderInfo" >
                    <Record Type="UI.HeaderInfoType">
                        <PropertyValue Property="TypeName" String="{@i18n>employee}"/>
                        <PropertyValue Property="TypeNamePlural" String="{@i18n>employees}"/>
                        <PropertyValue Property="Title">
                            <Record Type="UI.DataField">
                                <PropertyValue Property="Value" Path="FirstName"/>
                                <PropertyValue Property="Label" String="{@i18n>firstName}"/>
                            </Record>
                        </PropertyValue>
                        <PropertyValue Property="Description">
                            <Record Type="UI.DataField">
                                <PropertyValue Property="Value" Path="LastName"/>
                                <PropertyValue Property="Label" String="{@i18n>lastName}"/>
                            </Record>
                        </PropertyValue>
                        <PropertyValue Property="ImageUrl" String="sap-icon://employees"/>
                        <PropertyValue Property="TypeImageUrl" String="PhotoPath"/>
                    </Record>
                </Annotation>
            </Annotations>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
